<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Load department" parallel="tests" thread-count="6">	
	
	 <listeners>	  
		  <listener class-name="test.Listeners"/>
		  	  </listeners>
	

<!-- Basically we can do suite level parameterization(for opening website) or folder level parameterization(for opening or loging in any partilvuar folder personal loan,car loan etc.) -->
	
  <test  name="Personal Loan">
	 
	  
	  <parameter name="URL" value="PersonalLoan.com"/>
    <classes>
      <class name="test.day1">
      
      <methods>
         <include name="demo"/>
      </methods>    
       
      </class> 
        
       <class name="test.day2"/>
      <class name="test.day4"/>
    </classes>
  </test>
  
 
 
 
   <test name="Car loan">
   <parameter name="URL" value="carloan.com"/>
   <classes>
	   
	   <class name ="test.day3">
		   
	   
	   <methods>
		   <exclude name ="LoginAPICarLoan"/>
	   </methods>
	   
	   </class>
   </classes>
   </test>
   
   
</suite>


